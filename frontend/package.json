    "typescript": "^4.3.5",
    "web-vitals": "^0.2.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "migrate": "cd api && source venv/bin/activate && python manage.py create_db && python manage.py db init && python manage.py db migrate",
    "migrate:reset": "cd api && source venv/bin/activate && python manage.py drop_db && rm -rf ./migrations/* && rmdir ./migrations",
    "start-api": "cd api && source venv/bin/activate && venv/bin/flask run",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
